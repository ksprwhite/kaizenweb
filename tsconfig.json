{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es6",
        "moduleResolution": "node",
        "emitDecoratorMetadata": true,
        "esModuleInterop": true,
        "experimentalDecorators": true,
        "noEmitOnError": false,
        "allowJs": false
    },
    "exclude": [
        "node_modules",
        "typings"
    ]
}